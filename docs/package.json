    "carbon-components-svelte": "../",
    "clipboard-copy": "^3.1.0",
    "fs-extra": "^9.0.1",
    "mdsvex": "^0.8.8",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.4.2",
    "polka": "^0.5.2",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.21.0",
    "remark-slug": "^6.0.0",
    "sirv": "^1.0.6",
    "svelte": "3.29.0",
    "svelte-hmr": "0.11.1",
    "svelte-preprocess": "^4.3.2",
    "svite": "0.7.2"
  },
  "routify": {
    "routifyDir": ".routify",
